{
  "name": "swaggerdoc",
  "displayName": "swaggerdoc",
  "description": "This extension is perfect to generating swagger documentation mucho faster",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "swaggerdoc.generateDoc",
          "when": "editorHasSelection"
        }
      ]
    },
    "commands": [
      {
        "command": "swaggerdoc.generateDoc",
        "title": "SwaggerDoc: Generate Swagger Documentation"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "acorn": "^8.10.0",
    "acorn-walk": "^8.2.0"
  }
}
